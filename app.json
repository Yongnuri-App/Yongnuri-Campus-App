{
  "expo": {
    "name": "Yongnuri",
    "slug": "Yongnuri",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "yongnuri",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiBaseUrl": "http://192.168.0.108:8080",
      "ADMIN_ID": "202178028@yiu.ac.kr",
      "ADMIN_PW": "1234",
      "profilePath": "/mypage"
    }
  }
}
