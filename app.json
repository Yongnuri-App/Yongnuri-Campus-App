{
  "expo": {
    "name": "Yongnuri",
    "slug": "Yongnuri",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "yongnuri",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.vzhsge.Yongnuri"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiBaseUrl": "http://10.0.2.2:8080",
      "websocketUrl": "ws://10.0.2.2:8080/ws-stomp",
      "ADMIN_ID": "admin@example.com",
      "ADMIN_PW": "admin1234!",
      "profilePath": "/mypage",
      "eas": {
        "projectId": "35c6b5a2-3b07-4564-b316-e9fbb94cf4d9"
      }
    }
  }
}
